@model Usuario
<form asp-controller="Usuario" asp-action="CrearUsuario">
    
  <div class="mb-3">
    <label asp-for="@Model.NombreUsuario" class="form-label">Nombre</label>
    <input  asp-for="@Model.NombreUsuario" class="form-control" >
    <div class="form-text"></div>
  </div>
  <button type="submit" class="btn btn-primary">Enviar</button>
  <button><a asp-controller="Usuario" asp-action="Index">Cancelar</a></button>
</form>