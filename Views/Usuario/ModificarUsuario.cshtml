@model Usuario

<form asp-controller="Usuario" asp-action="ModificarUsuario" asp-route-idUsu="@Model.Id">
    <input hidden asp-for="@Model.Id">
    <div class="mb-3" class="form-group">
        <label asp-for="@Model.NombreUsuario" class="form-label"></label>
        <input asp-for="@Model.NombreUsuario" class="form-control"/>
    </div>


    <button type="submit" class="btn btn-info">Editar</button>
</form>
